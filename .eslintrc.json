{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": [
      "error",
      {
        "functions": "never"
      }
    ],
    "no-underscore-dangle": [
      "error",
      {
        "allow": true
      }
    ]
  }
}
